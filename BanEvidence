_G.Username = "Player username here"

if game.Players[_G.Username].leaderstats.Slaps.Value < 11500 then
game.Players[_G.Username].leaderstats.Slaps.Value = "11500"
end
game.Players[_G.Username].leaderstats.Glove.Value = "el gato"
while task.wait(.06) do
game.Players[_G.Username].leaderstats.Slaps.Value = game.Players[_G.Username].leaderstats.Slaps.Value + 1
end
